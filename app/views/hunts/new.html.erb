<h1>New hunt</h1>

<%= form_for(@hunt) do |f| %>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name%>
  </p>
  <p>
    <%= f.label :url %><br />
    <%= f.text_field :url %>
  </p>
  <p>
    <%= f.label :captain %><br />
    <%= f.text_field :captain %>
  </p>
  <p>
    <%= f.label :editor %><br />
    <%= f.text_field :editor %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', hunts_path %>
